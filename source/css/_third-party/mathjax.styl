.article
  mjx-container
    font-family: $fontfamily-code
    background: $color-mathjax
    padding: $gap $gap * 0.5
    border-radius: $border-codeblock
    min-width: 0 !important
  mjx-container[jax="CHTML"][display="true"], .has-jax
    overflow: auto hidden
  mjx-container + br
    display: none
